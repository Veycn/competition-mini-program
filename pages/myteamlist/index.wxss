/* pages/myteamlist/index.wxss */
.myteamlist{
  width: 100%;
  height: 100%;
  background: #f8f8f8;
  box-sizing: border-box;
  padding: 15rpx 15rpx 0;
}
.item{
  width: 100%;
  box-sizing: border-box;
  padding: 10rpx;
  border-radius: 10rpx;
  background: #fff;
  margin-bottom: 10rpx;
}
.item > .text{
  color: #666;
  font-size: 34rpx;
}
.name{
  color: #ccb458;
}